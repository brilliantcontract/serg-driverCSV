{
  "name": "1",
  "version": "1.2",
  "description": "Scrapes data from pages based on user-defined JSON instructions",
  "manifest_version": 3,
  "permissions": [
    "tabs",
    "scripting",
    "activeTab"
  ],
  "host_permissions": [
    "https://brilliantcontract.net:8443/*",
    "<all_urls>"
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.js"
  }
}